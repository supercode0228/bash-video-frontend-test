{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["react-app", "prettier"],
	"parserOptions": {
		"ecmaVersion": 11
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"printWidth": 80,
				"traillingComma": "es6",
				"semi": true,
				"jsxSingleQuote": true,
				"singleQuote": true,
				"useTabs": true
			}
		]
	}
}
